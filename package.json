{
  "name": "thestorm-ui-v4",
  "version": "0.0.1",
  "description": "THESTORM GUI v4 - for QL v2",
  "main": "index.js",
  "private": "true",
  "scripts": {
    "build": "rm -rf ./dist/* && webpack --mode production --env production",
    "devbuild": "rm -rf ./public/* && ENVIRONMENT=development webpack --watch --mode=development",
    "startLATER": "npm run start:devbuild",
    "start": "ENVIRONMENT=development webpack serve --mode development --open",
    "start:devbuild": "npm run devbuild",
    "start:local": "webpack-dev-server --mode development --progress",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Developers",
  "license": "UNLICENSED",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.0.0",
    "miragejs": "^0.1.41",
    "react-refresh": "^0.9.0",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-shell-plugin-next": "^2.2.2"
  }
